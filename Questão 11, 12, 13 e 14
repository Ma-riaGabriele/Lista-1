14*)#include <stdio.h>

int main ()

{float dias;

float pagamento;

float pagamentofinal;

puts("Digite o número de dias trabalhados: ");

scanf("%f", &dias);

pagamento = 50.25 * dias;

if(dias >10 && dias<=20) {

pagamento = pagamento + pagamento * 0.2;

pagamentofinal pagamento pagamento * 0.1;

printf("seu pagamento é: %f", pagamentofinal);

}

else if (dias>20) {

pagamento = pagamento + pagamento * 0.3;

pagamentofinal= pagamento pagamento * 0.1;

printf("seu pagamento é: %f", pagamentofinal);

}

else { pagamentofinal = pagamento -0.1* pagamento;

printf("seu pagamento é: %f", pagamentofinal);
}
return 0;

}

}
