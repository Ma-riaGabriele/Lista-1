12*)#include <stdio.h>

int main(void) {
    int A, B, aux;

    printf("Digite o valor para A: ");
    scanf("%d", &A);
    printf("Digite o valor para B: ");
    scanf("%d", &B);

    aux = A;
    A = B;
    B = aux;

    printf("Apos a troca:\n");
    printf("A = %d\n", A);
    printf("B = %d\n", B);

    return 0;
}


